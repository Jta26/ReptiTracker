{
    "extends": [
      "tslint-react-recommended"
    ],
    "rules": {
      "ordered-imports": [
        true
      ],
      "quotemark": [
        true,
        "Double",
        "jsx-single",
        "avoid-escape"
      ],
      "semicolon": [
        true,
        "always"
      ],
      "member-access": [
        false
      ],
      "member-ordering": [
        false
      ],
      "trailing-comma": [
        true,
        {
          "singleline": "never",
          "multiline": "always"
        }
      ],
      "linebreak-style": [false, "CRLF"],
      "no-empty": false,
      "no-submodule-imports": false,
      "no-implicit-dependencies": false,
      "no-constant-condition": false,
      "triple-equals": [
        true,
        "allow-undefined-check"
      ],
      "ter-indent": [
        true,
        4,
        {
          "SwitchCase": 1
        }
      ],
      "no-duplicate-imports": false,
      "jsx-alignment": true,
      "jsx-no-bind": true,
      "jsx-no-lambda": true
    }
  }